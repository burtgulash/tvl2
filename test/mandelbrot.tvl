\c_abs := \x -> \(\a:\b := x | (a * a + b * b) ** (1/2)) |
\c_add := \x.\y -> \(\a:\b,\c:\d := x,y | (a + c):(b + d) ) |
\c_mul := \x.\y -> \(\a:\b,\c:\d := x,y | ((a * c) - (b * d)):((a * d) + (b * c)) ) |


\iterate := \x.\y -> \(
	\c := (x * (1.75f.) - 1):(1) |

	\loop := \z.\n -> \(
		((z c_abs. pr. < 2) and (n > 0)) ? (
			(z c_mul z c_add c) loop (n - 1)
		) :| (
			n / 30
		)
	) |


	(0:0) loop 35
)
